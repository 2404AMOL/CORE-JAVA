class K
{
	public static void main(String[] args) 
	{
		for (int r=1;r<=7;r++)
		{
			for (int c=1;c<=6;c++)
			{
				if (c==1 || (r==4 && (c>=1 && c<=3) || (r<=4 && c+r==7) || (r>=4 && (c+r>=7 && c+r==2))))
				{
					System.out.print(" "+"*");
				}
				else
				{
					System.out.print(" "+ " ");
				}
			}
			System.out.println();
		}
	}
}
