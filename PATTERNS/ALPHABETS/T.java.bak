class T 
{
	public static void main(String[] args) 
	{
		int n=15;
		for (int r=1;r<=n;r++)
		{
			for (int c=1;c<=n;c++)
			{
				if (r==1 || c==n/2)
				{
					System.out.print("*"+" ");
				}
				else
				{
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}
}
