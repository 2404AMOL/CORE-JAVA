class R 
{
	public static void main(String[] args) 
	{
		for (int r=1;r<=9;r++)
		{
			for (int c=1;c<=6;c++)
			{
				if ((c==1 && r>1) || (r==1 && c>1 && c<=5) || (c==6 && r>1 && r<=4) || (r==5 && c<=5) || (r>=5 && c+r%2=0))
				{
					System.out.print("*"+" ");
				}
				else
				{
					System.out.print(" "+" ");
				}
			}
			System.out.println();
		}
	}
}
