//QNO7:-Write A Program To Print All The Odd Numbers Between O to 25
class OddNum0to25 
{
	public static void main(String[] args) 
	{
		int count=0;

		for (int num=1;num<=25;num++)
		{
			if (num%2==1)
			{
				System.out.println(num);
				count++;
			}
		}
		System.out.println("Total Odd Number is : "+count);
	}
}
