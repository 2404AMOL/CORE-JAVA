//QNO4:-Write A Program To Print All The Even Numbers Between O to 25
class EvenNum0to25 
{
	public static void main(String[] args) 
	{
		int count=0;

		for (int num=1;num<=25;num++)
		{
			if (num%2==0)
			{
				System.out.println(num);
				count++;
			}
		}
		System.out.println("Total Even Num is : "+count);
	}
}
